{
  "$schema": "http://json.schemastore.org/swcrc",
  "jsc": {
    "target": "es2015",
    "parser": {
      "syntax": "typescript"
    },
    "minify": {
      "compress": {
        // Don't change the names of functions exported as default
        "inline": false,
        "reduce_vars": false,

        // Remove console logs
        "drop_console": true
      }
    }
  },
  "sourceMaps": true
}
